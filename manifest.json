{
    "manifest_version": 3,
    "name": "AI Blog Generator",
    "version": "1.0",
    "description": "Generate blogs using AI with one click!",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["http://127.0.0.1:5000/*"]
  }
  